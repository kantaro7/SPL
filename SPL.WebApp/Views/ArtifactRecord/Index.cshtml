@model SPL.WebApp.ViewModels.ArtifactRecordViewModel

@{ ViewData["Title"] = "Index"; }


<br />
<div class="progress" style="height:5px">
    <div class="progress-bar" role="progressbar" style="width: 100%;" aria-valuenow="100" aria-valuemin="0" aria-valuemax="100"></div>
</div>

<br />
<div class="row">
    <div class="col-md-12">
        <div class="btn-group" role="group">
            <button id="btnRequest" type="button" style="margin-right:8px; font-weight:600;" class="btn btn-primary btn-sm">Consultar</button>
            <button id="btnSave" type="button" style="margin-right:8px; font-weight:600; " class="btn btn-primary btn-sm" disabled>Guardar</button>
            <button id="btnLoadDataSidco" style="margin-right:8px; font-weight:600; " type="button" class="btn btn-primary btn-sm" disabled>Datos SIDCO</button>
            <button id="btnClear" type="button" style="margin-right:8px; font-weight:600; " class="btn btn-secondary btn-sm"><i class="bi-eraser" style="font-size: 1.3rem;"></i></button>
        </div>
    </div>
 
</div>
<br />
<div class="row">
    <div class="form-group">
        @(Html.ValidationSummary(true, "", new { @class = "alert alert-danger" }))

    </div>
    <div class="col-md-4">
     
         
            <div class="form-floating mb-3">
                <input  onkeypress="return soloLetrasYNumerosSinEspacio(event)" asp-for="NoSerie" class="form-control form-control-sm" placeholder="NoSerie">
                <label asp-for="NoSerie"></label>
                <span id="NoSerieSpand" class="text-danger"></span>
                    
            </div>
            <div class="col-sm-6" style="padding-bottom:8px">
                <span id="lbIsFromSPL" style="display:none" class="badge rounded-pill bg-primary col-md-auto">Diseño registrado en SPL</span>
                <span id="lbIsFromSIDCO" style="display:none" class="badge rounded-pill bg-secondary col-md-auto">Diseño registrado en SIDCO</span>
            </div>
      
    </div>
    <div id="loader" class="col-md-2" style="padding-bottom:8px;display:none;padding-top: 12px;">
           <div class="spinner-border" role="status">
     <span class="visually-hidden">Espere...</span>
   </div> 


    </div> 
</div>


<div class="row" >
    <div class="col-md-12">
        <div class="container-fluid">

            <kendo-tabstrip name="tabstrip" style="background:white !important ">
                <popup-animation>
                    <open effects="fade:in" />
                </popup-animation>


                <items>


                    <tabstrip-item onclick="onchangeFocusTabs(this)" text="Generales" id="GENERAL" selected="true" icon-class="k-icon k-i-signature">

                        <content>

                            <div class="container">
                                <form asp-action="#" kendo-validator="true" id="form_general_data">

                                    <div class="row">
                                        <div class="col">
                                            <div class="form-floating mb-3">
                                                <input asp-for="Descripcion" class="form-control form-control-sm" placeholder="Descripcion">
                                                <label asp-for="Descripcion"></label>
                                                <span asp-validation-for="Descripcion" class="text-danger"></span>
                                            </div>
                                            <div class="form-floating mb-3">
                                                <input asp-for="CustomerName" class="form-control form-control-sm" placeholder="CustomerName">
                                                <label asp-for="CustomerName"></label>
                                                <span asp-validation-for="CustomerName" class="text-danger"></span>
                                            </div>
                                            <div class="form-floating mb-3">
                                                <input asp-for="PoNumeric" class="form-control form-control-sm" placeholder="PoNumeric">
                                                <label asp-for="PoNumeric"></label>
                                                <span asp-validation-for="PoNumeric" class="text-danger"></span>
                                            </div>



                                            <div class="col mb-3">
                                                <label class="form-label" asp-for="Typetrafoid"></label>
                                                @Html.DropDownListFor(c => c.Typetrafoid, (IEnumerable<SelectListItem>)ViewBag.TypetrafoItems, new { @class = "form-select form-select-sm" })
                                                <span asp-validation-for="Typetrafoid" class="text-danger"></span>
                                            </div>
                                            <div class="col mb-3">
                                                <label class="form-label" asp-for="StandardId"></label>
                                                @Html.HiddenFor(c => c.Norma)
                                                @Html.DropDownListFor(c => c.StandardId, (IEnumerable<SelectListItem>)ViewBag.NormaItems, new { @class = "form-select form-select-sm" })
                                                <span asp-validation-for="StandardId" class="text-danger"></span>
                                            </div>
                                            <div class="col mb-3">
                                                <label class="form-label" asp-for="LanguageId"></label>
                                                @Html.DropDownListFor(c => c.LanguageId, (IEnumerable<SelectListItem>)ViewBag.LanguageItems, new { @class = "form-select form-select-sm" })
                                                <span asp-validation-for="LanguageId" class="text-danger"></span>
                                            </div>


                                            <div class="col mb-3">
                                                <label class="form-label" asp-for="OilType"></label>
