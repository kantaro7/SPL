        async function GetEquivalenciaJSON(position) {

            var path = '/ArtifactRecord/GetEquivalencia';
            var conexion = "";
            if (position == 1) {
                conexion = $('#IdConexionAltaTension').val()
            }
            else if (position == 2) {
                conexion = $('#IdConexionBajaTension').val()

            }
            else if (position == 3) {
                conexion = $('#IdConexionSegundaBaja').val()

            }
            else if (position == 4) {

                conexion = $('#IdConexionTercera ').val()
            }
            var url = new URL(domain + path),
                params = {
                    pTipoConexion: conexion,
                    pclaveIdioma: $('#ClaveIdioma').val(),

                }

            Object.keys(params).forEach(key => url.searchParams.append(key, params[key]))

            const response = await fetch(url);

            if (response.ok && response.status === 200) {

                const data = await response.json();
                return data;
            }

            else {

                return null;
            }


        }



