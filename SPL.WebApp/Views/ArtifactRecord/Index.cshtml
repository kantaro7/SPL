                                                <label asp-for="Phases"></label>
                                                <span asp-validation-for="Phases" class="text-danger"></span>
                                            </div>
                                            <div class="form-floating mb-3">
                                                <input asp-for="Frecuency" id="Frecuency" class="form-control form-control-sm" placeholder="Frecuency" onkeypress="return soloNumeros(event)">
                                                <label asp-for="Frecuency"></label>
                                                <span asp-validation-for="Frecuency" class="text-danger"></span>
                                            </div>
                                            <div>
                                                <div class="row">
                                                    <div class="col">
                                                        <div class="form-floating mb-3">
                                                            <input asp-for="AltitudeF1" id="AltitudeF1" class="form-control form-control-sm" placeholder="AltitudeF1" onkeypress="return soloNumeros(event)">
                                                            <label asp-for="AltitudeF1"></label>
                                                            <span asp-validation-for="AltitudeF1" class="text-danger"></span>
                                                        </div>
                                                    </div>
                                                    <div class="col">
                                                        <div class="form-floating mb-3">
                                                            <input asp-for="AltitudeF2" class="form-control form-control-sm">
                                                        </div>
                                                    </div>
                                                </div>
                                            </div>

                                            <div class="col mb-3">
                                                <label class="form-label" asp-for="TipoUnidad"></label>
                                                @Html.DropDownListFor(c => c.TipoUnidad, (IEnumerable<SelectListItem>)ViewBag.TipoUnidadItems, new { @class = "form-select form-select-sm" })
                                                <span asp-validation-for="TipoUnidad" class="text-danger"></span>
                                            </div>

                                            <div class="col mb-3">
                                                <label class="form-label" asp-for="NormaEquivalente"></label>
                                                @Html.DropDownListFor(c => c.NormaEquivalente, (IEnumerable<SelectListItem>)ViewBag.NormaEquivalenteItems, new { @class = "form-select form-select-sm", @onchange="refreshRules(this);"  })
                                                <span asp-validation-for="NormaEquivalente" class="text-danger"></span>
                                            </div>
                                            <div class="col mb-3">
                                                <label class="form-label" asp-for="ClaveIdioma"></label>
                                                @Html.DropDownListFor(c => c.ClaveIdioma, (IEnumerable<SelectListItem>)ViewBag.ClaveIdiomaItems, new { @class = "form-select form-select-sm", @onchange="refreshRules(this);" })
                                                <span asp-validation-for="ClaveIdioma" class="text-danger"></span>
                                            </div>


                                            <div class="col mb-3">
                                                <label class="form-label" asp-for="ApplicationId"></label>
                                                @Html.DropDownListFor(c => c.ApplicationId, (IEnumerable<SelectListItem>)ViewBag.ApplicationidItems, new { @class = "form-select form-select-sm" })
                                                <span asp-validation-for="ApplicationId" class="text-danger"></span>
                                            </div>

                                        </div>
                                    </div>
                                    <br />
                                    <div class="row">
                                        <div class="col">
                                            <fieldset>
                                                <legend>Requerimientos Especiales, Excepto Reactores</legend>
                                            </fieldset>
                                            <div class="col mb-3">
                                                <label class="form-label" asp-for="PolarityId"></label>
                                                @Html.DropDownListFor(c => c.PolarityId, (IEnumerable<SelectListItem>)ViewBag.PolarityItems, new { @class = "form-select form-select-sm" })
                                                <span asp-validation-for="PolarityId" class="text-danger"></span>
                                            </div>
                                            <div class="form-floating mb-3">
                                                <input asp-for="PolarityOther" class="form-control form-control-sm" placeholder="PolarityOther">
                                                <label asp-for="PolarityOther"></label>
                                                <span asp-validation-for="PolarityOther" class="text-danger"></span>
                                            </div>
                                            <div class="col mb-3">
                                                <label class="form-label" asp-for="DesplazamientoAngular"></label>
                                                @Html.DropDownListFor(c => c.DesplazamientoAngular, (IEnumerable<SelectListItem>)ViewBag.DesplazamientoAngularItems, new { @class = "form-select form-select-sm" })
                                                <span asp-validation-for="DesplazamientoAngular" class="text-danger"></span>
                                            </div>
                                        </div>
                                    </div>
                                </form>

                            </div>
                        </content>
                    </tabstrip-item>

                    <tabstrip-item onclick="onchangeFocusTabs(this)" text="Características" id="CHARACTERISTIC" icon-class="k-icon k-i-list-unordered">
